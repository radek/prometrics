{
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "curly": 2,
    "eqeqeq": 1,
    "complexity": [2, 15],
    "no-unused-vars": 1,
    "no-undef": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 1,
    "quotes": [1, "single", "avoid-escape"],
    "no-loop-func": 1,
    "no-unreachable": 2,
    "no-spaced-func": 1,
    "semi": [2, "always"],
    "no-extra-semi": 1,
    "keyword-spacing": 1,
    "valid-typeof": 2,
    "guard-for-in": 1,
    "no-native-reassign": 2,
    "no-extend-native": 2,
    "no-var": 1,
    "no-eval": 2,
    "no-sparse-arrays": 2,
    "no-param-reassign": 1,
    "no-multiple-empty-lines": 1,
    "wrap-iife": [1, "outside"],
    "indent": [1, 2],
    "no-use-before-define": 1,
    "max-depth": [1, 5],
    "new-cap": [1, { "capIsNew": true }],
    "no-caller": 2,
    "no-empty": [1, { "allowEmptyCatch": true }],
    "no-new": 1,
    "no-console": 1,
    "no-func-assign": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-trailing-spaces": 1,
    "no-dupe-args": 2,
    "no-whitespace-before-property": 1,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "prefer-const": 1,
    "semi-spacing": 1,
    "space-before-blocks": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-with": 2,
    "no-continue": 2,
    "space-before-function-paren": [1, { "anonymous": "never", "named": "never" }]
  }
}
